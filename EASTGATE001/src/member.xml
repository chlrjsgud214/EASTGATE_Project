<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="members">
	<select id="select" parameterType="string" resultType="Member">
		select * from member where mid=#{id}
	</select>
	<insert id="insert" parameterType="member">
		insert into member values(admin_mcode.nextval, 'g', #{mid}, #{mpass}, #{mname}, #{resinum}, #{tel}, #{addr}, sysdate, 'n')
	</insert>
	<update id="update" parameterType="member">
		update member set mpass=#{mpass}, mname=#{mname}, address=#{address}, tel=#{tel} where mid=#{mid} 
	</update> 
	<update id="delete" parameterType="string">
		update member set mdel='y' where mid=#{mid}
	</update> 
</mapper>
