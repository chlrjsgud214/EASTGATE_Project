<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="orderns">
	<select id="select" parameterType="string" resultType="Orders">
		select * from Orders where ocode=#{ocode}
	</select>
	<insert id="insert" parameterType="orders">
		insert into orders values(#{pcode}, #{mcode}, orders_ocode.nextval, #{ocount}, n, #{oprice}, sysdate)
	</insert>
</mapper>